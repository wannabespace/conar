{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "ignorePatterns": [
    "**/node_modules",
    "**/dist",
    "**/out",
    "**/release",
    "**/.turbo",
    "**/.output",
    "**/routeTree.gen.ts",
    "**/migrations.json",
  ],
}
