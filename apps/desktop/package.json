  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite -c vite.electron.config.ts dev",
    "build:vite": "electron-vite -c vite.electron.config.ts build",
    "build:electron": "electron-builder -p always",
    "build:todesktop": "todesktop build",
    "check-types": "tsc",
    "test:start": "vite -c vite.test.config.ts build --mode test && vite -c vite.test.config.ts preview --mode test",
    "test:unit": "bun test ./src",
    "test:e2e": "playwright test --headed",
    "drizzle:generate": "drizzle-kit generate && bun run ./scripts/compile-migrations.ts",
    "drizzle:generate-custom": "drizzle-kit generate --custom && bun run ./scripts/compile-migrations.ts"
  },
