    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/xai": "^2.0.23",
    "@ai-sdk/xai1": "npm:@ai-sdk/xai@1.2.18",
    "@conar/connection": "workspace:*",
    "@conar/shared": "workspace:*",
    "@hono/arktype-validator": "^2.0.1",
    "@hono/trpc-server": "^0.4.0",
    "@hono/zod-validator": "^0.7.3",
    "@orpc/server": "catalog:",
